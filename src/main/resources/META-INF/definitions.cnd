<jmix = 'http://www.jahia.org/jahia/mix/1.0'>
<jnt = 'http://www.jahia.org/jahia/nt/1.0'>
<mix = 'http://www.jcp.org/jcr/mix/1.0'>
<nt = 'http://www.jcp.org/jcr/nt/1.0'>
<saml2nt = 'http://www.jahia.org/saml2/nt/1.0'>
<saml2 = 'http://www.jahia.org/saml2/1.0'>

[jnt:samlLogin] > jnt:content, jmix:basicContent

[saml2nt:settings] > nt:base, jmix:unversionedBasemetadata
 - saml2:identityProviderPath (string) nofulltext
 - saml2:relyingPartyIdentifier (string) nofulltext
 - saml2:incomingTargetUrl (string) nofulltext
 - saml2:spMetaDataLocation (string) nofulltext
 - saml2:keyStoreLocation (string) nofulltext
 - saml2:keyStorePass (string) nofulltext
 - saml2:privateKeyPass (string) nofulltext

[saml2nt:manageSettings] > jnt:content, jmix:siteComponent, mix:title, jmix:studioOnly

